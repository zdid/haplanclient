{
  "name": "home-assistant-floorplan-client",
  "version": "1.0.0",
  "private": true,
  "description": "Client application for Home Assistant floor plan visualization",
  "main": "dist/App.js",
  "scripts": {
    "build": "node build.js",
    "build:tsc": "tsc",
    "build:vite": "vite build",
    "watch": "tsc --watch",
    "dev": "vite",
    "preview": "vite preview",
    "start": "npm run build && serve dist -p 8080",
    "dev:full": "concurrently \"npm run dev\" \"serve public -p 8080\"",
    "lint": "eslint src/**/*.ts",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "prepare": "husky install"
  },
  "dependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitest/coverage-v8": "^0.34.0",
    "concurrently": "^8.0.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.0",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.0.0",
    "prettier": "^3.0.0",
    "serve": "^14.0.0",
    "vite": "^6.4.1",
    "vitest": "^0.34.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
